{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "../dist.tmp/node.d.ts",
  "bundledPackages": ["ethers"],
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "../lib/node.d.ts"
  },
  "compiler": {
    "tsconfigFilePath": "./tsconfig.dts.json",
    "skipLibCheck": true
  },
  "projectFolder": ".",
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "extractorMessageReporting": {
      "ae-missing-release-tag": {
        "logLevel": "none"
      }
    }
  }
}
